<project xmlns="http://maven.apache.org/DECORATION/1.7.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd"
	name="PNML to NUPN Converter">

	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="right" />

	<skin>
		<groupId>io.github.devacfr.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>2.0.0-beta2</version>
	</skin>

	<bannerRight>
		<name>LIP6</name>
		<src>images/logo-Lip6.png</src>
		<href>https://www.lip6.fr</href>
	</bannerRight>

	<custom>
		<reflowSkin>
			<localResources>true</localResources>
			<smoothScroll>true</smoothScroll>
			<!-- bootswatch-readable -->
			<theme>bootswatch-flatly</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<bootstrapVersion>4.1.3</bootstrapVersion>
      		<bootswatchVersion>4.1.3</bootswatchVersion>
			<brand>
				<name>PNML to NUPN Converter</name>
				<href>http://pnml.lip6.fr/pnml2nupn/index.html</href>
			</brand>
			<slogan>Free Tool to transform your PNML P/T Documents into NUPN
				(Format of Caesar.bdd)</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			
			<navbar filterMenu="Download|Documentation" cssClass="navbar-light bg-light border-bottom"/>
			<bottomNav maxSpan="9">
				<column>Overview</column>
				<column>Download|Documentation</column>
				<column>Find out more</column>
				<column>Contribute|reports|modules</column>
			</bottomNav>
			<bottomDescription>
			<![CDATA[
				 <span class="color-highlight">PNML to NUPN Converter</span> is a free command-line tool to transform your <a href="http://www.pnml.org">Petri Net Markup Language</a> Documents into 
				NUPN, the format of the <a href="http://cadp.inria.fr/man/caesar.bdd.html#sect4">Caesar.bdd tool</a>.
			 ]]>
			</bottomDescription>
			<pages>
				<index project="fr.lip6.move.pnml2nupn">
					<titleTemplate>PNML to NUPN Converter</titleTemplate>
					<shortTitle>Welcome</shortTitle>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<body />
						<sidebar />
						<thumbs>2</thumbs>
						<columns>2</columns>
					</sections>
				</index>
				<changelog project="fr.lip6.move.pnml2nupn">
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<sections>
						<body />
					</sections>
				</changelog>
				<usage project="fr.lip6.move.pnml2nupn">
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<sections>
						<body />
					</sections>
				</usage>
				<developer-info>
					<navbar filterMenu="inherit" cssClass="navbar-dark bg-dark " />
					<toc>sidebar</toc>
				</developer-info>
			</pages>
		</reflowSkin>
	</custom>

	<body>
		<head>
			<![CDATA[
				<link rel="author" href="https://lip6.fr/Lom-Messan.Hillah"/>
			]]>
		</head>
		<menu name="Overview" inherit="top">
			<item name="Home" href="index.html" />
			<item name="Changelog" href="changelog.html" />
		</menu>
		<menu name="Download" inherit="top">
			<item name="Download" href="download.html" />
		</menu>
		<menu name="Documentation" inherit="bottom">
			<item name="Usage" href="usage.html" />
		</menu>
		<menu name="Find out more" inherit="bottom">
			<item name="Caesar.bdd"
				href="http://cadp.inria.fr/man/caesar.bdd.html" />
			<item name="PNML Framework" href="http://pnml.lip6.fr/" />
			<item name="PNML.org" href="http://www.pnml.org/" />
			<item name="Project on GitHub"
				href="https://github.com/lip6/pnml2nupn" />
		</menu>
		<menu name="Contribute" inherit="bottom">
			<item name="Contribute" href="contribute.html" />
			<item name="Issues"
				href="https://github.com/lip6/pnml2nupn/issues" />
			<item name="Fork on GitHub"
				href="https://github.com/lip6/pnml2nupn" />
		</menu>
		<menu name="Maven documentation" ref="reports" inherit="bottom" />
	</body>

</project>
